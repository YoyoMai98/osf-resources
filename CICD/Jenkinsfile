pipeline {
    agent any

    stages {
        stage("build") {
            steps {
                echo "build stage..."
                sh "cd CICD"
                sh "./mvnw clean install -DskipTests"
            }
        }

        stage("test") {
            steps {
                echo "build stage..."
                sh "cd CICD"
                sh "./mvnw test"
            }
        }

        stage("deploy") {
            steps {
                echo "build stage..."
            }
        }
    }
}